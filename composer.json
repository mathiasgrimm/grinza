{
    "name": "mathiasgrimm/grinza",
    "authors": [
        {
            "name": "Mathias Grimm",
            "email": "mathiasgrimm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "vlucas/phpdotenv": "^2.4",
        "mathiasgrimm/arraypath": "^2.0",
        "mathiasgrimm/x": "^1.4",
        "satooshi/php-coveralls": "^1.0",
        "psr/log": "^1.0"
    },

    "require-dev": {
        "phpunit/phpunit": "^5.0.0"
    },

    "autoload-dev": {
        "exclude-from-classmap": [
        ],
        "classmap": [
            "tests/TestCase.php"
        ]
    },

    "autoload": {
        "psr-4": {
            "Grinza\\": "src/@lib"
        },
        "classmap": [
            "src/@lib/functions.php"
        ]
    }


}
